{
  "name": "vuex-ts",
  "version": "4.1.0",
  "description": "Strongly-typed Vuex modules built for high-complexity stores and high-scalability needs.",
  "author": "Ian K Smith <smithki707@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smithki/vuex-ts.git"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "dev": "npm-run-all -s clean:dist compile_watch",
    "build": "npm-run-all -s clean:dist compile",
    "compile": "microbundle build src/index.ts --external vuex --name VuexTs",
    "compile_watch": "microbundle watch src/index.ts --external vuex --name VuexTs",
    "example": "parcel example/index.html --out-dir example-dist",
    "clean": "npm-run-all -s clean:*",
    "clean:dist": "rimraf dist && rimraf coverage",
    "clean:example-dist": "rimraf example-dist",
    "clean:cache": "rimraf .rts2_cache_* .cache",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@ikscodes/tslint-config": "^5.3.1",
    "microbundle": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "vue": "^2.5.0",
    "vuex": "^3.1.0"
  }
}
